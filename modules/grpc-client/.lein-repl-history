(require '[lambdaisland.uri :as li])
(li/uri "http://localhost:8080")
(def i1 (li/uri "http://localhost:8080"))
(def i2 (li/uri "localhost"))
(keys i1)
(select-keys i1 [:host :port :scheme])
(select-keys i2 [:host :port :scheme])
(def i2 (li/uri "http://localhost"))
(select-keys i2 [:host :port :scheme])
